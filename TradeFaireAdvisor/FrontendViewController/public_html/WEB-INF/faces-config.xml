<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
  <application>
    <default-render-kit-id>oracle.adf.rich</default-render-kit-id>
  </application>
  <navigation-rule>
    <from-view-id>/RegistrationPage.jsf</from-view-id>
    <navigation-case>
      <from-outcome>RegistrationOk</from-outcome>
      <to-view-id>/MainInterests.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/MainInterests.jsf</from-view-id>
    <navigation-case>
      <from-outcome>VisitorRequestedForSecondaryInterests</from-outcome>
      <to-view-id>/SecondaryInterests.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>VisitorChoosenMainInterests</from-outcome>
      <to-view-id>/VisitorPrioriotyForSelectedInterests.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/SecondaryInterests.jsf</from-view-id>
    <navigation-case>
      <from-outcome>VisitorRequestForMainInterests</from-outcome>
      <to-view-id>/MainInterests.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>VisitorChoosenSecondaryInterests</from-outcome>
      <to-view-id>/VisitorPrioriotyForSelectedInterests.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/VisitorPrioriotyForSelectedInterests.jsf</from-view-id>
    <navigation-case>
      <from-outcome>VisitorSendPrioListOfInterests</from-outcome>
      <to-view-id>/priotirezedExhibitorsResultForVisitor.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/priotirezedExhibitorsResultForVisitor.jsf</from-view-id>
    <navigation-case>
      <from-outcome>VisitorRequestListOfPublicEvents</from-outcome>
      <to-view-id>/ListOfPublicEventsDuringTheTraidFair.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>VisitorRequestListOfBookmarkedEvents</from-outcome>
      <to-view-id>/ListOfVisitorBookmarkedEvents.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/ListOfPublicEventsDuringTheTraidFair.jsf</from-view-id>
    <navigation-case>
      <from-outcome>VisitorRequestListOfPrioExhibitors</from-outcome>
      <to-view-id>/priotirezedExhibitorsResultForVisitor.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>VisitorRequestPageOfPublicBookmarkedEventsFromPublicEventsPage</from-outcome>
      <to-view-id>/ListOfVisitorBookmarkedEvents.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/ListOfVisitorBookmarkedEvents.jsf</from-view-id>
    <navigation-case>
      <from-outcome>VisitorRequestListOfPrioExhibitors</from-outcome>
      <to-view-id>/priotirezedExhibitorsResultForVisitor.jsf</to-view-id>
    </navigation-case>
    <navigation-case>
      <from-outcome>VisitorRequestListOfPublicEventsFromBookmarksOfPublicEvents</from-outcome>
      <to-view-id>/ListOfPublicEventsDuringTheTraidFair.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>
</faces-config>